<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <ViewSettingsDialog
        confirm="onSetFilter"
        filterItems="{/invoices}">
        <filterItems>
            <ViewSettingsFilterItem
                text="{invoiceid}"
                key="{businessArea}">
            </ViewSettingsFilterItem>
        </filterItems>
    </ViewSettingsDialog>
</core:FragmentDefinition>









