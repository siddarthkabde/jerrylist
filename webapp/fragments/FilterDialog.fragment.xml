<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog confirm="handleFilterDialogConfirm">
		<filterItems>
			<ViewSettingsFilterItem text="ClassText" key="ClassKey" multiSelect="true" items="{/Class}">
				<items>
					<ViewSettingsItem key="{invoiceid}" text="{businessArea}"></ViewSettingsItem>
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem text="AddressText" key="AddressKey" multiSelect="true" items="{/Address}">
				<items>
					<ViewSettingsItem key="{invoiceid}" text="{businessArea}"></ViewSettingsItem>
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem text="Price" key="Price" multiSelect="false">
				<items>
					<ViewSettingsItem text="Less Than 100" key="Price___LE___100___X"/>
					<ViewSettingsItem text="Between 100 and 1000" key="Price___BT___100___1000"/>
					<ViewSettingsItem text="More Than 1000" key="Price___GT___1000___X"/>
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>