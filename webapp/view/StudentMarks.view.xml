<mvc:View controllerName="loyolabdn.controller.StudentMarks" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core">
	<Page id="employeeMarksPage" title="Student Marks" showNavButton="true" navButtonPress="onNavBack" class="sapUiResponsiveContentPadding">
		<l:VerticalLayout class="sapUiContentPadding" width="100%">
			<l:content>
				<Label text="Product" labelFor="productInput"/>
				<Input id="productInput" type="Text" placeholder="Search Anything ..." showSuggestion="true" showValueHelp="true"
					valueHelpRequest="handleValueHelp" suggestionItems="{/ProductCollection}">
					<suggestionItems>
						<core:Item text="{Name}"/>
					</suggestionItems>
				</Input>
			</l:content>
		</l:VerticalLayout>
	</Page>
</mvc:View>