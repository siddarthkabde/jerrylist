<mvc:View
  height="100%"
  controllerName="loyolabdn.controller.Launchpad"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m">
  <Page id="_IDEGen_page0"
    showHeader="True"
    enableScrolling="true" >
    <TileContainer
      id="container"
      tileDelete="handleTileDelete"
      tiles="{/TileCollection}">
      <StandardTile id="_IDEGen_standardtile0"
        icon="sap-icon://{icon}"
        type="{type}"
        number="{number}"
        numberUnit="{numberUnit}"
        title="{title}"
        info="{info}"
        press="tilePress"
        infoState="{infoState}" />
    </TileContainer>
    <footer>
      <Toolbar id="_IDEGen_toolbar0">
        <ToolbarSpacer id="_IDEGen_toolbarspacer0"/>
          <Button id="_IDEGen_button0" text="Edit" press="handleEditPress" />
          <Button id="_IDEGen_button1" text="Busy state" press="handleBusyPress" />
        <ToolbarSpacer id="_IDEGen_toolbarspacer1"/>
      </Toolbar>
    </footer>
  </Page>
</mvc:View>