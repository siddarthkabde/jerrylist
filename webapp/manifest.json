{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "jerrylist",
		"type": "component",
		"i18n": "i18n/i18n.properties",
		"title": "{{AppTitle}}",
		"description": "{{AppDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-DOC",
		"dataSources": {  }
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		},
		"supportedThemes": ["sap_bluecrystal"]
	},

	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "jerrylist.view.App",
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.m": {
					"minVersion": "1.30.0"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings":{
					"bundleName": "jerrylist.i18n.i18n"
				}
			}
		},
		"routing":{
			"config":{
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "jerrylist.view",
				"controlId": "app"
			},
			"routes":[{
				"pattern":"",
				"name":"home",
				"target":["master", "emptyView"]
			},{
				"pattern":"contact/{contactId}",
				"name": "contactdetail",
				"target": ["master", "detail"]
				}],
			"targets":{
				"master":{
					"viewName": "Master",
					"controlAggregation": "masterPages"
				},
				"emptyView":{
					"viewName": "EmptyView",
					"controlAggregation": "detailPages"
				},
				"detail":{
					"viewName": "Detail",
					"controlAggregation": "detailPages"
				}
			}
		}
	}
}
