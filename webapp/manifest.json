{
	"_version": "1.12.0",
	"sap.app": {
		"id": "loyolabdn",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"apps": {
				"uri": "model/tiles.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "loyolabdn.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "loyolabdn.i18n.i18n"
				}
			},
			"apps": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "apps"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}],
			"js": [{
				"uri": "./util/xlsx.js"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "loyolabdn.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false

			},
			"routes": [{
				"name": "appHome",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": "home"
			}, {
				"pattern": "employees",
				"name": "employeeList",
				"target": "employees"
			}, {
				"pattern": "PatternFileUpload",
				"name": "fileUpload",
				"target": "TargetFile"
			}, {
				"pattern": "StudentMarks",
				"name": "studentMarks",
				"target": "TargetStudentMarks"
			}, {
				"pattern": "Students/{Adm}",
				"name": "studentDetails",
				"target": "TargetStudent"
			}],
			"targets": {
				"home": {
					"viewId": "home",
					"viewName": "Launchpad",
					"viewLevel": 1
				},
				"employees": {
					"viewId": "employeeList",
					"viewName": "StudentTableView",
					"viewLevel": 2
				},
				"TargetFile": {
					"viewId": "fileUpload",
					"viewName": "FileUpload",
					"viewLevel": 2
				},
				"TargetStudentMarks": {
					"viewId": "studentMarks",
					"viewName": "StudentMarks",
					"viewLevel": 2
				},
				"TargetStudent": {
					"viewId": "studentDetails",
					"viewName": "StudentDetails",
					"viewLevel": 3
				}
			}
		}
	}
}